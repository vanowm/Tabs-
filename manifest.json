{
  "background": {
    "persistent": true,
    "scripts": [
      "include/common.js", "background.js"
    ]
  },
  "description": "Various controls for tabs",
  "icons": {
    "128": "ui/icons/icon_128.png",
    "64": "ui/icons/icon_64.png",
    "48": "ui/icons/icon_48.png",
    "32": "ui/icons/icon_32.png",
    "24": "ui/icons/icon_24.png",
    "16": "ui/icons/icon_16.png"
  },
  "manifest_version": 2,
  "name": "Tabs+",
  "options_page": "ui/options.html",
  "browser_action": {},
  "default_locale": "en",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "sessions"
  ],
  "version": "0.0.4"
}